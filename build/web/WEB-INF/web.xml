<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>RegistroUsuario</servlet-name>
        <servlet-class>cl.inacap.controlador.RegistroUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>cl.inacap.controlador.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlAdmin</servlet-name>
        <servlet-class>cl.inacap.controlador.ControlAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlUsuario</servlet-name>
        <servlet-class>cl.inacap.controlador.ControlUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistroUsuario</servlet-name>
        <url-pattern>/RegistroUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlAdmin</servlet-name>
        <url-pattern>/ControlAdmin</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>ControlUsuario</servlet-name>
        <url-pattern>/ControlUsuario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
